{
    "configurations": [
        {
            "name": "AVR (ATMega328P)",
            "includePath": [
                "${workspaceFolder}/lib"
            ],
            "defines": [
                "BAUD=9600",
                "F_CPU=16000000"
            ],
            "compilerPath": "/usr/bin/avr-gcc",
            "compilerArgs": [
                "-mmcu=atmega328p",
                "-Os"
            ],
            "cStandard": "c11",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}
